<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id="person1" class="quiz05.Person">
		<property name="name" value="남편"/>
		<property name="gender" value="남"/>
	</bean>
	<bean id="person2" class="quiz05.Person">
		<property name="name" value="와이프"/>
		<property name="gender" value="여"/>
	</bean>
	<bean id = "honeymoon1" class="quiz05.HoneyMoon">
		<property name="city"  value="하와이"/>
		<property name="husband" ref="person1"/>
		<property name="wife" ref="person2"/>
	
	</bean>

</beans>
