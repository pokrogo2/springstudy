	<!-- namespaces 탭에서 옵션 p선택하면 property 지정을 bean 태그의 속성으로 처리가능
		<property> 지정을 bean태그의 속성으로 처리
	
	  -->
	  
<!-- <bean id = "객체명" class="클래스" p:필드명 ="값" -->



	<!-- Engine engine = new  -->

	<bean id ="engine" class="ex02_xml.Engine" p:type="디젤" p:cc="1950" p:hp="235" p:efficiency="12.5"></bean>
